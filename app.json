{
  "expo": {
    "name": "Techno Titans Scouting",
    "slug": "TitanScoutingApp",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/Images/logo_512x512.png",
    "splash": {
      "image": "./assets/Images/logo_512x512.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "technotitans.scoutingapp"
    },
    "description": "testing in progress",
    "android": {
      "package": "technotitans.scoutingapp",
      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends"
        }
      ]
    ]
  }
}
